[TYPECHECK]
# Generates objects members for classes to avoid corresponding error
generated-members=objects

[MASTER]
# Ignores database migration files
ignore=migrations

[MESSAGES CONTROL]
# Ignores warnings about locally disables errors and ignoring entire files
disable=I0011, I0013

[REPORTS]
# Simplify pylint reports
reports=no

[SIMILARITIES]
# Ignore imports when computing similarities.
ignore-imports=yes